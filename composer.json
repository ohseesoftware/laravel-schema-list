{
    "name": "ohseesoftware/laravel-schema-list",
    "description": "List tables, columns, and indices for your database connections.",
    "keywords": [
        "ohseesoftware",
        "laravel-schema-list"
    ],
    "homepage": "https://github.com/ohseesoftware/laravel-schema-list",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Owen Conti",
            "email": "owen@ohseemedia.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/console": ">=7.0",
        "illuminate/database": ">=7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^8.0",
        "squizlabs/php_codesniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "OhSeeSoftware\\LaravelSchemaList\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OhSeeSoftware\\LaravelSchemaList\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "OhSeeSoftware\\LaravelSchemaList\\LaravelSchemaListServiceProvider"
            ]
        }
    }
}
